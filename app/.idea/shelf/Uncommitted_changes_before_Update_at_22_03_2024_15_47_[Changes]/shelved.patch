+@import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500;600;700&display=swap');
+
+*{
+    margin: 0;
+    padding: 0;
+    box-sizing: border-box;
+    font-family: 'Montserrat', sans-serif;
+}
+
+body{
+    background-color: #c9d6ff;
+    background: linear-gradient(to right, rgb(255, 234, 196), rgb(194, 194, 255));
+    display: flex;
+    align-items: center;
+    justify-content: center;
+    flex-direction: column;
+    height: 100vh;
+}
+
+.container{
+    display: flex;
+    gap: 200px;
+    margin-top: -20px;
+}
+
+.container input{
+    background-color: #eee;
+    border: none;
+    margin: 20px 0;
+    padding: 15px 15px;
+    font-size: 13px;
+    border-radius: 8px;
+    width: 550px;
+    outline: none;
+}
+
+.input {
+    margin-top: 40px;
+}
+
+h1 {
+    text-align: center;
+    margin-bottom: 50px;
+    font-size: 70px;
+}
+
+.bottone button{
+    background-color: rgb(61, 61, 94);
+    color: #fff;
+    font-size: 12px;
+    padding: 10px 45px;
+    border: 1px solid transparent;
+    border-radius: 8px;
+    font-weight: 600;
+    letter-spacing: 0.5px;
+    text-transform: uppercase;
+    margin-top: 70px;
+    cursor: pointer;
+    margin-left: 210px;
+}
+
+h3 {
+    font-weight: 100;
+    font-size: 15px;
+}
+
+img {
+    margin-top: 50px;
+}
+
+p {
+    text-align: center;
+}
